{
  "kind": "collectionType",
  "collectionName": "shop_items",
  "info": {
    "singularName": "shop-item",
    "pluralName": "shop-items",
    "displayName": "ShopItem",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true
    },
    "Price": {
      "type": "float",
      "required": true
    },
    "Description": {
      "type": "string",
      "required": true
    },
    "Photo": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "Quantity": {
      "type": "integer"
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "Date": {
      "type": "date",
      "required": true
    },
    "Discount": {
      "type": "integer"
    },
    "payment": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::payment.payment",
      "inversedBy": "shop_items"
    }
  }
}
